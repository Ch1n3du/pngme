#[derive(PartialEq, )]
pub struct ChunkType {
    bytes: [u8; 4]
}

impl ChunkType {
    fn get_bit_five(byte: u8) -> u8 {
        (byte << 3) >> 7
    }

}

impl TryFrom<[u8; 4]> for ChunkType {

    fn try_from(bytes: [u8; 4]) -> Result<Self, Self::Error> {
        Ok(ChunkType {bytes})
    }
}




